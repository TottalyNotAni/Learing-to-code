import math
print("Lūdzu noradīt koefficentus a, b un c!")
a=float(input("A:"))
b=float(input("B:"))
c=float(input("C:"))

print("A: " + str(a) + " " + "B: " + str(b) + " " + "C: " + str(c))
D = b*b - 4*a*c
print("D:" +str(D))
if D < 0: print("Saknu nav")
if D > 0:
        x1 = (-b + math.sqrt(D)) / (2*a)
        x2 = (-b - math.sqrt(D)) / (2*a)
        print("Ir divas saknes")
        print(x1)
        print(x2)
if D==0:
    x = (-b) / (2*a)
    print("Ir viena sakne")
    print(x)
